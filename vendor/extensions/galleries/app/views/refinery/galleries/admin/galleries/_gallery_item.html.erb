<% items.each do |item| %>
    <% next unless item.image.present?  %>
    <div class="one-image item_<%= item.id %>">
      <%= image_fu(item.image, '150x150#') %>
      <%= link_to "remove", refinery.galleries_admin_gallery_remove_item_path(gallery.id, item.id), data:{confirm: 'Are you sure?'}, remote: true, class:'button', style:'position: absolute; margin: 5px;' %>
    </div>
<% end if items.present?%>